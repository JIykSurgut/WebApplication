@model ZDL.Models.AppUser

@{
    ViewBag.Title = "Удалить пользователя";
}

<h3>Удалить пользователя</h3>

<h5>Вы действительно хотите удалить пользователя?</h5>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Логин")
        </dt>

        <dd>
            @Html.DisplayFor(m => m.UserName)
        </dd>
    </dl>

    @using (Html.BeginForm())
    {
        <div class="form-actions no-color">
            <input type="submit" value="Удалить" class="btn btn-default" /> |
            @Html.ActionLink("Вернуться", "Index")
        </div>
    }
</div>